<idea-plugin>
    <id>com.pytest</id>
    <name>Pytest</name>
    <version>0.0.2</version>
    <vendor email="support@pytest.store" url="http://pytest.store">Victor Titor</vendor>

    <description><![CDATA[
      Pytest framework support
    ]]></description>

    <change-notes><![CDATA[
      <ul><li>Go to fixture declaration, which is defined in test file, conftest files or plugins</li>
      <li>The trial is available until January 1, 2018</li></ul>
    ]]>
    </change-notes>

    <idea-version since-build="173" until-build="173.*"/>

    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.python</depends>

    <extensions defaultExtensionNs="com.intellij">
        <gotoDeclarationHandler implementation="com.pytest.MyGotoDeclarationHandler"/>
    </extensions>
</idea-plugin>
