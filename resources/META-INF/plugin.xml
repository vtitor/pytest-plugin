<idea-plugin>
    <id>com.pytest</id>
    <name>Pytest</name>
    <version>1.0.0</version>
    <vendor email="vtitor.edumix@gmail.com" url="https://github.com/vtitor/pytest-plugin">Victor Titor</vendor>

    <description><![CDATA[
      Pytest framework support
    ]]></description>

    <change-notes><![CDATA[
      <ul><li>Add support for custom fixture names</li>
      <li>Compatibility with latest PyCharm (182.*) builds</li></ul>
    ]]>
    </change-notes>

    <idea-version since-build="173" until-build="182.*"/>

    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.python</depends>

    <extensions defaultExtensionNs="com.intellij">
        <gotoDeclarationHandler implementation="com.pytest.MyGotoDeclarationHandler"/>
    </extensions>
</idea-plugin>
