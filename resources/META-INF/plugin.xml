<idea-plugin>
    <id>com.pytest</id>
    <name>Pytest</name>
    <version>0.0.4</version>
    <vendor email="support@pytest.store" url="http://pytest.store">Victor Titor</vendor>

    <description><![CDATA[
      Pytest framework support
    ]]></description>

    <change-notes><![CDATA[
      <ul><li>Compatibility with latest PyCharm (181.*) builds</li></ul>
    ]]>
    </change-notes>

    <idea-version since-build="173" until-build="181.*"/>

    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.python</depends>

    <extensions defaultExtensionNs="com.intellij">
        <gotoDeclarationHandler implementation="com.pytest.MyGotoDeclarationHandler"/>
    </extensions>
</idea-plugin>
